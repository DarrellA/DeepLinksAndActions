<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".WarningActivity">

    <Button
            android:text="You sure you want to see it?"
            android:layout_width="180dp"
            android:layout_height="168dp"
            android:onClick="goAhead"
            android:id="@+id/button"
            android:layout_marginEnd="117dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="114dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="437dp" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="126dp" app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintVertical_bias="1.0"/>
    <Button
            android:text="On second thought, no."
            android:layout_width="180dp"
            android:layout_height="168dp"
            android:onClick="nope"
            android:id="@+id/button2"
            android:layout_marginEnd="116dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="115dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="181dp" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="382dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>